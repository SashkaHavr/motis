{
  "name": "rsl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "dev-host": "vite --host",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "serve-host": "vite preview --host",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "fix-lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src",
    "format-check": "prettier --check src",
    "ts-check": "tsc",
    "update-protocol": "node --trace-uncaught tools/protocol.js && eslint src/api/protocol --ext .ts --fix && prettier --write src/api/protocol"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@radix-ui/react-dropdown-menu": "^0.1.6",
    "@radix-ui/react-tooltip": "^0.1.7",
    "@visx/axis": "^2.6.0",
    "@visx/grid": "^2.6.0",
    "@visx/responsive": "^2.8.0",
    "@visx/scale": "^2.2.2",
    "@visx/shape": "^2.4.0",
    "@visx/stats": "^2.2.2",
    "date-fns": "^2.28.0",
    "downshift": "^6.1.7",
    "jotai": "^1.6.3",
    "lodash-es": "^4.17.21",
    "optics-ts": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.29.0",
    "react-query": "^3.34.19",
    "react-virtuoso": "^2.9.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@limegrass/eslint-plugin-import-alias": "^1.0.2",
    "@tailwindcss/forms": "^0.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-react": "^1.3.1",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  }
}
